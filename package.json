{
    "name": "topsjf",
    "version": "3.3.1",
    "description": "MY.笔记",
    "license": "MIT",
    "scripts": {
        "build:vite": "vuepress-vite build docs",
        "dev:vite": "vuepress-vite dev docs",
        "dev:vite-clean": "vuepress-vite dev docs --clean-cache",
        "test": "echo demo test out!",
        "commit": "git-cz",
        "lint-md": "markdownlint **/*.md --ignore node_modules --fix",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vuepress/client": "2.0.0-beta.53",
        "vue": "^3.2.45",
        "vuepress": "2.0.0-beta.53",
        "vuepress-plugin-search-pro": "2.0.0-beta.132",
        "vuepress-theme-hope": "2.0.0-beta.132",
        "vuepress-vite": "2.0.0-beta.53"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "commitizen": "^4.2.5",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^8.0.2",
        "markdownlint-cli": "^0.32.2"
    },
    "packageManager": "pnpm@7.17.1",
    "engines": {
        "node": ">=14",
        "npm": ">=8",
        "pnpm": ">=7"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@algolia/client-search",
                "webpack",
                "sass-loader"
            ]
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
